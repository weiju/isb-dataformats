<experiment name="Fe chelation" date="2003-02-10">

  <predicate category='species' value='Helicobacter pylori'/>
  <predicate category='strain'  value='SS1'/>
  <predicate category='perturbation' value='environmental:Fe:chelation'/>

  <link type="journalArticle" url="http://iai.asm.org/cgi/content/full/71/11/6510" />

  <dataset status='primary' type='log 10 ratios'>
    <uri> feChelation.ratio </uri>
  </dataset>


  <condition alias='Log Chelate T.1'>
    <variable name='growth phase' value='exponential' />
    <variable name='time' value='000' units='minutes' />
  </condition>

  <condition alias='Log Chelate T.2'>
    <variable name='growth phase' value='exponential' />
    <variable name='time' value='030' units='minutes' />
  </condition>

  <condition alias='Log Chelate T.3'>
    <variable name='growth phase' value='exponential' />
    <variable name='time' value='060' units='minutes' />
  </condition>

  <condition alias='Log Chelate T.4'>
    <variable name='growth phase' value='exponential' />
    <variable name='time' value='090' units='minutes' />
  </condition>

  <condition alias='Log Chelate T.5'>
    <variable name='growth phase' value='exponential' />
    <variable name='time' value='120' units='minutes' />
  </condition>

  <condition alias='Stat Chelate T.1'>
    <variable name='growth phase' value='stationary' />
    <variable name='time' value='000' units='minutes' />
  </condition>

  <condition alias='Stat Chelate T.2'>
    <variable name='growth phase' value='stationary' />
    <variable name='time' value='030' units='minutes' />
  </condition>

  <condition alias='Stat Chelate T.3'>
    <variable name='growth phase' value='stationary' />
    <variable name='time' value='060' units='minutes' />
  </condition>

  <condition alias='Stat Chelate T.4'>
    <variable name='growth phase' value='stationary' />
    <variable name='time' value='090' units='minutes' />
  </condition>

  <condition alias='Stat Chelate T.5'>
    <variable name='growth phase' value='stationary' />
    <variable name='time' value='120' units='minutes' />
  </condition>

 </experiment>
